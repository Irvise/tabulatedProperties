FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      mut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    middlePlane
    {
        type            symmetryPlane;
    }
    solidContact
    {
        type            mutURoughWallFunction;
        roughnessConstant  0.5;
        roughnessFactor    1;
        roughnessHeight    14.5e-06;
        value           uniform 1e-4;
    }

}


// ************************************************************************* //
