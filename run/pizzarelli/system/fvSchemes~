FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default steadyState;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;

    div(phi,U)      bounded Gauss MUSCL;
    div(phi,K)      bounded Gauss MUSCL;
    div(phi,h)      bounded Gauss MUSCL;
    div(phi,nuTilda)  bounded Gauss MUSCL;
    div((muEff*dev2(T(grad(U))))) Gauss linear;
    div((p*U))        Gauss linear;
    div((devRhoReff&U)) Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default         no;
    p;
}


// ************************************************************************* //
