FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    inlet
    {
        type            compressible::turbulentMixingLengthFrequencyInlet;
        mixingLength    6.75e-06;
        phi             phi;
        k               k;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    middlePlane
    {
        type            symmetryPlane;
        value           uniform 0;    
    }        
    sideWall
    {
        type            zeroGradient;
        value           uniform 0;
    }
    hot
    {
        type            zeroGradient;
        value           uniform 0;
    }
    cold
    {
        type            zeroGradient;
        value           uniform 0;
    }
}


