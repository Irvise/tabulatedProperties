FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 2.5e-5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    sideWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    middlePlane
    {
        type            symmetryPlane;
    }
    hot
    {
        type            fixedValue;
        value           uniform 0;
    }
    cold
    {
        type            fixedValue;
        value           uniform 0;
    }
}


