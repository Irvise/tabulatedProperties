FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 37.5;

boundaryField
{
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 37.5;
    }
    outlet
    {
        type            zeroGradient;
    }
    middlePlane
    {
        type            symmetryPlane;
        value           uniform 0;    
    }        
    sideWall
    {
        type            compressible::kqRWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
    hot
    {
        type            compressible::kqRWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
    cold
    {
        type            compressible::kqRWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
}


