FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 80e5;

boundaryField
{
    inlet
    {
        type            totalPressure;
        p0              uniform 9e+06;
        gamma           1.75;
        rho             rho;
        value           uniform 9e6;
    }
    outlet
    {
        type            fixedValue;      
        value           uniform 7e+06;
    }
    middlePlane
    {
        type            symmetryPlane;  
    }        
    sideWall
    {
        type            zeroGradient;
    }
    hot
    {
        type            zeroGradient;
    }
    cold
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
