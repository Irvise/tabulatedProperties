FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 3e6;

boundaryField
{
    inlet
    {
        type            compressible::turbulentMixingLengthDissipationRateInlet; 
        mixingLength    6.75e-06;
        value           uniform 3e6;
    }
    outlet
    {
        type            zeroGradient;
    }  
    middlePlane
    {
        type            symmetryPlane;
        value           uniform 0;    
    }          
    sideWall
    {
        type            compressible::epsilonWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
    hot
    {
        type            compressible::epsilonWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
    cold
    {
        type            compressible::epsilonWallFunction;
        Prt             0.9;
        value           uniform 0;
    }
}


